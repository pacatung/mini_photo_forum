<h1>edit.html</h1>

<%= form_for @photo, :url => photo_path do |f| %>


  <%= render :partial => 'form', :locals => { :f => f } %>
  <br>
  <p>
    Origin Image
  </p>
  <%#= image_tag( @photo.pic(:medium) ) %>
  <%= link_to image_tag(@photo.pic.url(:medium)), @photo.pic.url, :target => "_blank" %>

  <br>
  <br>
  <%= f.submit "Update" %>
  <%= link_to 'Cancel', photos_path, :class => "btn btn-danger" %>

<% end %>