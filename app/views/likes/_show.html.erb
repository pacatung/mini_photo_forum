<%= photo.likes_count %> peoples like.

<% photo.like_users.each do |u| %>
  <%= link_to u.name, user_path(u) %>
<% end %>
<br>
<% if like %>
  <%= link_to "[Dislike]", photo_like_path(photo, like), :method => :delete, :remote => true %>
<% else %>
  <%= link_to "[Like]", photo_likes_path(photo), :method => :post, :remote => true %>
<% end %>
